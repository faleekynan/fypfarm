{
  "manifest_version": 2,
  "name": "FYP Scripts",
  "version": "0.5",
  "description": "",
  "permissions": [
    "tabs", "http://*/", "https://*/"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*","https://developers.facebook.com/*"],
      "js": ["/jquery-3.6.1.min.js", "main.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
    "default_icon": "logo.png"
  }
}
